<!--		
	Name: 		StatusHumanResourceExample_v1.0.xml
	Purpose: 	Defines an offer status example for the Netherlands.
	Author(s): 	SETU
	Standard:	SETU Standard for Ordering & Selection v1.0
	Last edit:	2020-11-25
-->
<HumanResource 	xmlns="http://ns.hr-xml.org/2007-04-15" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:nl="http://ns.setu.nl/2007-01" xsi:schemaLocation="http://ns.hr-xml.org/2007-04-15 ../../../hr-xml/SIDES/HumanResource.xsd http://ns.setu.nl/2007-01 ../../schemas/2007-01/HumanResourceAdditionalNL.xsd">
	<HumanResourceId>
		<IdValue/>
	</HumanResourceId>
	<HumanResourceStatus status="accepted"/>
	<ReferenceInformation>
		<StaffingSupplierId>
			<IdValue/>
		</StaffingSupplierId>
		<StaffingCustomerId>
			<IdValue/>
		</StaffingCustomerId>
		<OrderId idOwner="StaffingCustomer">
			<IdValue>121</IdValue>
		</OrderId>			
	</ReferenceInformation>
	<ResourceInformation>
		<PersonName/>
		<EntityContactInfo>
			<EntityName/>
			<ContactMethod/>
		</EntityContactInfo>
	</ResourceInformation>
	<Profile/>
	<Preferences/>
	<UserArea>
		<nl:HumanResourceAdditionalNL>
			<nl:OfferId>7654323</nl:OfferId>
			<nl:ReasonOfChange>Accepted</nl:ReasonOfChange>
		</nl:HumanResourceAdditionalNL>	
	</UserArea>
</HumanResource>
