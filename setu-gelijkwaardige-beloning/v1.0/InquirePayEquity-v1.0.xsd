<?xml version="1.0" encoding="UTF-8"?>
<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema" xmlns:sth="https://ns.semantic-treehouse.nl/message" elementFormDefault="qualified" attributeFormDefault="unqualified" targetNamespace="https://ontology.setu.nl" xmlns:ns0="https://ontology.setu.nl" xmlns:setu="https://ontology.setu.nl/planning" xmlns:hropen="https://hropenstandards.org/schema/xml">
  <!--===== Root element definition =====-->
  <xs:element name="InquiryPayEquity">
    <xs:complexType>
      <xs:sequence>
        <xs:element name="id">
          <xs:annotation>
            <xs:documentation>
              <sth:definition>The id is a unique identifier (within the context of the customer) assigned to a specific compensation regulations.</sth:definition>
              <sth:note>It is important not to confuse this with the **document number**, which is the identifier of this message.</sth:note>
            </xs:documentation>
          </xs:annotation>
          <xs:complexType>
            <xs:sequence>
              <xs:element name="value">
                <xs:annotation>
                  <xs:documentation>
                    <sth:definition>The identifier.</sth:definition>
                  </xs:documentation>
                </xs:annotation>
                <xs:simpleType>
                  <xs:restriction base="xs:string"/>
                </xs:simpleType>
              </xs:element>
            </xs:sequence>
          </xs:complexType>
        </xs:element>
        <xs:element name="documentId">
          <xs:annotation>
            <xs:documentation>
              <sth:definition>Provides the Identifier(s) of the document being communicated. Uniquely identifies a Document within an organization.</sth:definition>
            </xs:documentation>
          </xs:annotation>
          <xs:complexType>
            <xs:sequence>
              <xs:element name="value">
                <xs:annotation>
                  <xs:documentation>
                    <sth:definition>The value of the unique identifier.</sth:definition>
                  </xs:documentation>
                </xs:annotation>
                <xs:simpleType>
                  <xs:restriction base="xs:string"/>
                </xs:simpleType>
              </xs:element>
              <xs:element name="schemeAgencyId">
                <xs:annotation>
                  <xs:documentation>
                    <sth:definition>The organization that issues the identifier. This organization is also responsible for unicity of the identifier within its own context.</sth:definition>
                  </xs:documentation>
                </xs:annotation>
                <xs:simpleType>
                  <xs:restriction base="xs:string">
                    <xs:enumeration value="Customer"/>
                    <xs:enumeration value="Supplier"/>
                  </xs:restriction>
                </xs:simpleType>
              </xs:element>
            </xs:sequence>
          </xs:complexType>
        </xs:element>
        <xs:element name="issued" minOccurs="0" type="xs:dateTime">
          <xs:annotation>
            <xs:documentation>
              <sth:definition>The datetime the document was issued.</sth:definition>
            </xs:documentation>
          </xs:annotation>
        </xs:element>
        <xs:element name="effectivePeriod">
          <xs:annotation>
            <xs:documentation>
              <sth:definition>The period from the start date up and including to the end date. This period indicates the validity of this specification. </sth:definition>
            </xs:documentation>
          </xs:annotation>
          <xs:complexType>
            <xs:sequence>
              <xs:element name="validFrom">
                <xs:annotation>
                  <xs:documentation>
                    <sth:definition>The start date of the validity period of this specification.</sth:definition>
                  </xs:documentation>
                </xs:annotation>
                <xs:simpleType>
                  <xs:restriction base="xs:date"/>
                </xs:simpleType>
              </xs:element>
              <xs:element name="validTo" minOccurs="0">
                <xs:annotation>
                  <xs:documentation>
                    <sth:definition>The end date of the validity period of this specification.</sth:definition>
                  </xs:documentation>
                </xs:annotation>
                <xs:simpleType>
                  <xs:restriction base="xs:date"/>
                </xs:simpleType>
              </xs:element>
            </xs:sequence>
          </xs:complexType>
        </xs:element>
        <xs:element name="customer">
          <xs:annotation>
            <xs:documentation>
              <sth:definition>The customer/hirer of the worker.</sth:definition>
            </xs:documentation>
          </xs:annotation>
          <xs:complexType>
            <xs:sequence>
              <xs:element name="id" minOccurs="0" maxOccurs="2">
                <xs:annotation>
                  <xs:documentation>
                    <sth:definition>A unique identifier which cannot be expressed as enumeration.</sth:definition>
                  </xs:documentation>
                </xs:annotation>
                <xs:complexType>
                  <xs:sequence>
                    <xs:element name="value">
                      <xs:annotation>
                        <xs:documentation>
                          <sth:definition>The identifier.</sth:definition>
                        </xs:documentation>
                      </xs:annotation>
                      <xs:simpleType>
                        <xs:restriction base="xs:string"/>
                      </xs:simpleType>
                    </xs:element>
                    <xs:element name="schemeAgencyId" minOccurs="0">
                      <xs:annotation>
                        <xs:documentation>
                          <sth:definition>The identification of the agency that manages the identifier scheme.</sth:definition>
                        </xs:documentation>
                      </xs:annotation>
                      <xs:simpleType>
                        <xs:restriction base="xs:string">
                          <xs:enumeration value="Customer"/>
                          <xs:enumeration value="Supplier"/>
                        </xs:restriction>
                      </xs:simpleType>
                    </xs:element>
                  </xs:sequence>
                </xs:complexType>
              </xs:element>
              <xs:element name="name" minOccurs="0">
                <xs:annotation>
                  <xs:documentation>
                    <sth:definition>Name of the customer</sth:definition>
                  </xs:documentation>
                </xs:annotation>
                <xs:simpleType>
                  <xs:restriction base="xs:string"/>
                </xs:simpleType>
              </xs:element>
              <xs:element name="legalId" maxOccurs="unbounded">
                <xs:annotation>
                  <xs:documentation>
                    <sth:definition>A government assigned identifier to distinguish the party (organization, person, or legal entity) to a transaction.</sth:definition>
                  </xs:documentation>
                </xs:annotation>
                <xs:complexType>
                  <xs:sequence>
                    <xs:element name="value">
                      <xs:annotation>
                        <xs:documentation>
                          <sth:definition>The identifier.</sth:definition>
                        </xs:documentation>
                      </xs:annotation>
                      <xs:simpleType>
                        <xs:restriction base="xs:string"/>
                      </xs:simpleType>
                    </xs:element>
                    <xs:element name="schemeAgencyId">
                      <xs:annotation>
                        <xs:documentation>
                          <sth:definition>The identification of the agency that manages the identifier scheme.</sth:definition>
                        </xs:documentation>
                      </xs:annotation>
                      <xs:simpleType>
                        <xs:restriction base="xs:string">
                          <xs:enumeration value="KvK"/>
                          <xs:enumeration value="OIN"/>
                          <xs:enumeration value="RSIN"/>
                        </xs:restriction>
                      </xs:simpleType>
                    </xs:element>
                  </xs:sequence>
                </xs:complexType>
              </xs:element>
              <xs:element name="personContacts">
                <xs:annotation>
                  <xs:documentation>
                    <sth:definition>Specific people at the party who are contacts. This is the person authorized to provide the information</sth:definition>
                  </xs:documentation>
                </xs:annotation>
                <xs:complexType>
                  <xs:sequence>
                    <xs:element name="name">
                      <xs:annotation>
                        <xs:documentation>
                          <sth:definition>A component for capturing summary or fine-grain data comprising a person's name.</sth:definition>
                        </xs:documentation>
                      </xs:annotation>
                      <xs:complexType>
                        <xs:sequence>
                          <xs:element name="formattedName">
                            <xs:annotation>
                              <xs:documentation>
                                <sth:definition>The formatted name of a person, as it would be written out together (for example, on a mailing label).</sth:definition>
                              </xs:documentation>
                            </xs:annotation>
                            <xs:simpleType>
                              <xs:restriction base="xs:string"/>
                            </xs:simpleType>
                          </xs:element>
                        </xs:sequence>
                      </xs:complexType>
                    </xs:element>
                    <xs:element name="communication" minOccurs="0">
                      <xs:annotation>
                        <xs:documentation>
                          <sth:definition>The communication channels (phone, email) to contact and reach the contact person.</sth:definition>
                        </xs:documentation>
                      </xs:annotation>
                      <xs:complexType>
                        <xs:sequence>
                          <xs:element name="phone" minOccurs="0">
                            <xs:annotation>
                              <xs:documentation>
                                <sth:definition>Array of phone information details.</sth:definition>
                              </xs:documentation>
                            </xs:annotation>
                            <xs:complexType>
                              <xs:sequence>
                                <xs:element name="formattedNumber">
                                  <xs:annotation>
                                    <xs:documentation>
                                      <sth:definition>Specifies details of an actual formatted number</sth:definition>
                                    </xs:documentation>
                                  </xs:annotation>
                                  <xs:simpleType>
                                    <xs:restriction base="xs:string"/>
                                  </xs:simpleType>
                                </xs:element>
                              </xs:sequence>
                            </xs:complexType>
                          </xs:element>
                          <xs:element name="email" minOccurs="0">
                            <xs:annotation>
                              <xs:documentation>
                                <sth:definition>Array of email information details.</sth:definition>
                              </xs:documentation>
                            </xs:annotation>
                            <xs:complexType>
                              <xs:sequence>
                                <xs:element name="address">
                                  <xs:annotation>
                                    <xs:documentation>
                                      <sth:definition>Actual email address</sth:definition>
                                    </xs:documentation>
                                  </xs:annotation>
                                  <xs:simpleType>
                                    <xs:restriction base="xs:string"/>
                                  </xs:simpleType>
                                </xs:element>
                              </xs:sequence>
                            </xs:complexType>
                          </xs:element>
                        </xs:sequence>
                      </xs:complexType>
                    </xs:element>
                    <xs:element name="roleCode" minOccurs="0">
                      <xs:annotation>
                        <xs:documentation>
                          <sth:definition>
The role of the contact person. In this case the inlenersbeloning is authorized by this contact person. Therefore the role code is mainly 'Authorized by'</sth:definition>
                        </xs:documentation>
                      </xs:annotation>
                      <xs:simpleType>
                        <xs:restriction base="xs:string"/>
                      </xs:simpleType>
                    </xs:element>
                    <xs:element name="positionTitle" minOccurs="0">
                      <xs:annotation>
                        <xs:documentation>
                          <sth:definition>The title of the position the contact person</sth:definition>
                        </xs:documentation>
                      </xs:annotation>
                      <xs:simpleType>
                        <xs:restriction base="xs:string"/>
                      </xs:simpleType>
                    </xs:element>
                  </xs:sequence>
                </xs:complexType>
              </xs:element>
            </xs:sequence>
          </xs:complexType>
        </xs:element>
        <xs:element name="labourAgreements" minOccurs="0">
          <xs:annotation>
            <xs:documentation>
              <sth:definition>This container indicates in which sector the staffing customer works and which specific collective labour agreement (CLA) and/or the own employment conditions scheme is leading for the provision of remuneration information.</sth:definition>
            </xs:documentation>
          </xs:annotation>
          <xs:complexType>
            <xs:sequence>
              <xs:element name="industryIdentifiers" minOccurs="0">
                <xs:annotation>
                  <xs:documentation>
                    <sth:definition>The types of industry to which the organization belongs. The Industry identifiers could be referenced to a standard taxonomy, such as the CBS - SBI 2008 (Standaard Bedrijfsindeling).</sth:definition>
                  </xs:documentation>
                </xs:annotation>
                <xs:complexType>
                  <xs:sequence>
                    <xs:element name="value" maxOccurs="unbounded">
                      <xs:annotation>
                        <xs:documentation>
                          <sth:definition>A unique identifier for the industry.</sth:definition>
                        </xs:documentation>
                      </xs:annotation>
                      <xs:simpleType>
                        <xs:restriction base="xs:string"/>
                      </xs:simpleType>
                    </xs:element>
                  </xs:sequence>
                </xs:complexType>
              </xs:element>
              <xs:element name="collectiveLabourAgreement" minOccurs="0">
                <xs:annotation>
                  <xs:documentation>
                    <sth:definition>Information about the collective labour agreement (CLA; CAO in Dutch) that determines the renumeration. In the Netherlands this can be the CLA of the ABU, NBBU or a specific CLA of the staffing customer.</sth:definition>
                  </xs:documentation>
                </xs:annotation>
                <xs:complexType>
                  <xs:sequence>
                    <xs:element name="name" type="xs:string">
                      <xs:annotation>
                        <xs:documentation>
                          <sth:definition>The name of the collective labour agreement (CLA).</sth:definition>
                        </xs:documentation>
                      </xs:annotation>
                    </xs:element>
                    <xs:element name="id">
                      <xs:annotation>
                        <xs:documentation>
                          <sth:definition>The identifier of the collective labour agreement (CLA).</sth:definition>
                        </xs:documentation>
                      </xs:annotation>
                      <xs:complexType>
                        <xs:sequence>
                          <xs:element name="value">
                            <xs:annotation>
                              <xs:documentation>
                                <sth:definition>A unique identifier for the collective labor agreement based on the CBS codelist.</sth:definition>
                              </xs:documentation>
                            </xs:annotation>
                            <xs:simpleType>
                              <xs:restriction base="xs:string"/>
                            </xs:simpleType>
                          </xs:element>
                        </xs:sequence>
                      </xs:complexType>
                    </xs:element>
                    <xs:element name="effectivePeriod" minOccurs="0">
                      <xs:annotation>
                        <xs:documentation>
                          <sth:definition>The period from the start date up and including to the end date. This period indicates the validity of the CLA.</sth:definition>
                        </xs:documentation>
                      </xs:annotation>
                      <xs:complexType>
                        <xs:sequence>
                          <xs:element name="validFrom" minOccurs="0">
                            <xs:annotation>
                              <xs:documentation>
                                <sth:definition>
The start date of the validity period of the collective labour agreement</sth:definition>
                              </xs:documentation>
                            </xs:annotation>
                            <xs:simpleType>
                              <xs:restriction base="xs:date"/>
                            </xs:simpleType>
                          </xs:element>
                          <xs:element name="validTo" minOccurs="0">
                            <xs:annotation>
                              <xs:documentation>
                                <sth:definition>
The end date of the validity period of the collective labour agreement</sth:definition>
                              </xs:documentation>
                            </xs:annotation>
                            <xs:simpleType>
                              <xs:restriction base="xs:date"/>
                            </xs:simpleType>
                          </xs:element>
                        </xs:sequence>
                      </xs:complexType>
                    </xs:element>
                    <xs:element name="basedOn" minOccurs="0" type="xs:string">
                      <xs:annotation>
                        <xs:documentation>
                          <sth:definition>A collective labor agreement (CLA) applies if the employer is a member of the industry association, if the CLA has been declared generally binding for the sector by the Minister of Social Affairs and Employment, or if it is declared applicable in the employee’s employment contract.</sth:definition>
                        </xs:documentation>
                      </xs:annotation>
                    </xs:element>
                  </xs:sequence>
                </xs:complexType>
              </xs:element>
              <xs:element name="customLabourAgreement" minOccurs="0" type="xs:boolean">
                <xs:annotation>
                  <xs:documentation>
                    <sth:definition>Indicator to determine whether a custom labor agreement is active.</sth:definition>
                  </xs:documentation>
                </xs:annotation>
              </xs:element>
            </xs:sequence>
          </xs:complexType>
        </xs:element>
        <xs:element name="positionProfile" minOccurs="0" maxOccurs="unbounded">
          <xs:annotation>
            <xs:documentation>
              <sth:definition>These are the position profiles for which the inlenersbeloning applies. From the salary table and other arrangements, you can also use these positions for the conditions of those arrangements.</sth:definition>
            </xs:documentation>
          </xs:annotation>
          <xs:complexType>
            <xs:sequence>
              <xs:element name="positionId">
                <xs:annotation>
                  <xs:documentation>
                    <sth:definition>The primary identifier of the position profile, typically from the system of record.</sth:definition>
                  </xs:documentation>
                </xs:annotation>
                <xs:complexType>
                  <xs:sequence>
                    <xs:element name="value">
                      <xs:annotation>
                        <xs:documentation>
                          <sth:definition>The value of the unique identifier.</sth:definition>
                        </xs:documentation>
                      </xs:annotation>
                      <xs:simpleType>
                        <xs:restriction base="xs:string"/>
                      </xs:simpleType>
                    </xs:element>
                  </xs:sequence>
                </xs:complexType>
              </xs:element>
              <xs:element name="positionTitle">
                <xs:annotation>
                  <xs:documentation>
                    <sth:definition>The title of the position, which is a short phrase describing the position as it would be listed on a business card or in a company directory. OED: An appellation attaching to a position in virtue of rank, function or office.</sth:definition>
                  </xs:documentation>
                </xs:annotation>
                <xs:simpleType>
                  <xs:restriction base="xs:string"/>
                </xs:simpleType>
              </xs:element>
              <xs:element name="referenceTitle" minOccurs="0">
                <xs:annotation>
                  <xs:documentation>
                    <sth:definition>This reference title provides a reference to the function/role as defined within the Collective Labor Agreement (CAO)</sth:definition>
                    <sth:note>If a collective labour agreement (CLA) applies and includes a job classification framework the reference titles from that framework must be specified in the position profiles.</sth:note>
                  </xs:documentation>
                </xs:annotation>
                <xs:simpleType>
                  <xs:restriction base="xs:string"/>
                </xs:simpleType>
              </xs:element>
              <xs:element name="workDescription" minOccurs="0">
                <xs:annotation>
                  <xs:documentation>
                    <sth:definition>The free text description of the work that this position normally requires.</sth:definition>
                  </xs:documentation>
                </xs:annotation>
                <xs:simpleType>
                  <xs:restriction base="xs:string"/>
                </xs:simpleType>
              </xs:element>
              <xs:element name="salaryScale" maxOccurs="unbounded" type="xs:string">
                <xs:annotation>
                  <xs:documentation>
                    <sth:definition>Specifies the salary scale that is applicable for this position profile. This element refers to the 'name' element in the corresponding position profile. </sth:definition>
                  </xs:documentation>
                </xs:annotation>
              </xs:element>
            </xs:sequence>
          </xs:complexType>
        </xs:element>
        <xs:element name="remuneration" maxOccurs="unbounded">
          <xs:annotation>
            <xs:documentation>
              <sth:definition>Details about the remuneration package, including salary tables based on the work duration. It includes one or more salary tables for a specific work duration or characteristics.</sth:definition>
              <sth:note>Uitleg van onderliggend voorbeeld:

Salaris tabel op basis van 40 uur per week met twee salaris schalen die ieder 3 verschillende tredes bevatten. De schalen zijn van toepassing op 1 of meerdere functies binnen de organisatie. De beloning wordt maandelijkst vastgesteld. In dit voorbeeld word ook werkervaring meegenomen bij inschaling, en is er een eenmalig verhoging op 01-01-2026 van 5 percentage op het jaarlijse salaris op basis van performance</sth:note>
            </xs:documentation>
          </xs:annotation>
          <xs:complexType>
            <xs:sequence>
              <xs:element name="effectivePeriod" minOccurs="0">
                <xs:annotation>
                  <xs:documentation>
                    <sth:definition>The period from the start date up and including to the end date. This period indicates the validity of this regulation. </sth:definition>
                  </xs:documentation>
                </xs:annotation>
                <xs:complexType>
                  <xs:sequence>
                    <xs:element name="validFrom" minOccurs="0">
                      <xs:annotation>
                        <xs:documentation>
                          <sth:definition>DateTime representation according ISO 8601 standard.</sth:definition>
                        </xs:documentation>
                      </xs:annotation>
                      <xs:simpleType>
                        <xs:restriction base="xs:date"/>
                      </xs:simpleType>
                    </xs:element>
                    <xs:element name="validTo" minOccurs="0">
                      <xs:annotation>
                        <xs:documentation>
                          <sth:definition>DateTime representation according ISO 8601 standard.</sth:definition>
                        </xs:documentation>
                      </xs:annotation>
                      <xs:simpleType>
                        <xs:restriction base="xs:date"/>
                      </xs:simpleType>
                    </xs:element>
                  </xs:sequence>
                </xs:complexType>
              </xs:element>
              <xs:element name="workDuration">
                <xs:annotation>
                  <xs:documentation>
                    <sth:definition>Specifies the work duration to which the salary table applies. The amount of work duration is always given as x hours per week. Where the x can be provided in the value element. </sth:definition>
                  </xs:documentation>
                </xs:annotation>
                <xs:complexType>
                  <xs:sequence>
                    <xs:element name="value">
                      <xs:annotation>
                        <xs:documentation>
                          <sth:definition>The value of the work duration, expressed as decimal. E.g., 40</sth:definition>
                        </xs:documentation>
                      </xs:annotation>
                      <xs:simpleType>
                        <xs:restriction base="xs:decimal"/>
                      </xs:simpleType>
                    </xs:element>
                  </xs:sequence>
                </xs:complexType>
              </xs:element>
              <xs:element name="intervalCode">
                <xs:annotation>
                  <xs:documentation>
                    <sth:definition>Defines the interval at which the remuneration of the salary table is determined, for example, per month, per four weeks, per week, etc.</sth:definition>
                  </xs:documentation>
                </xs:annotation>
                <xs:complexType>
                  <xs:sequence>
                    <xs:element name="value">
                      <xs:annotation>
                        <xs:documentation>
                          <sth:definition>The value of the interval code.</sth:definition>
                        </xs:documentation>
                      </xs:annotation>
                      <xs:simpleType>
                        <xs:restriction base="xs:string">
                          <xs:enumeration value="fourWeeks"/>
                          <xs:enumeration value="hour"/>
                          <xs:enumeration value="month"/>
                          <xs:enumeration value="week"/>
                        </xs:restriction>
                      </xs:simpleType>
                    </xs:element>
                  </xs:sequence>
                </xs:complexType>
              </xs:element>
              <xs:element name="hourlyWageConversion" minOccurs="0">
                <xs:annotation>
                  <xs:documentation>
                    <sth:definition>Information about the conversion to regular hourly wages.</sth:definition>
                    <sth:note>It is possible that hourly conversion rates differ on the step/cell level. If this is the case for one or more steps/cells, then add a salary table that describes the remuneration **per hour**.</sth:note>
                    <sth:note>If your employment conditions or collective labor agreement do not specify hourly wages or a clear calculation method, the hourly wage is calculated as follows: Monthly salary / (4.35 x normal working hours).</sth:note>
                  </xs:documentation>
                </xs:annotation>
                <xs:complexType>
                  <xs:sequence>
                    <xs:element name="hourlyWageFactor" minOccurs="0" type="xs:decimal">
                      <xs:annotation>
                        <xs:documentation>
                          <sth:definition>A multiplier used to calculate the hourly wage from another measure, such as a monthly salary.</sth:definition>
                          <sth:note>It is possible that hourly conversion rates differ on the step/cell level. If this is the case for one or more steps/cells, then add a salary table that describes the remuneration **per hour**.</sth:note>
                        </xs:documentation>
                      </xs:annotation>
                    </xs:element>
                    <xs:element name="hourlyWagePercentage" minOccurs="0" type="xs:decimal">
                      <xs:annotation>
                        <xs:documentation>
                          <sth:definition>The percentage of the hourly wage relative to the monthly salary.</sth:definition>
                          <sth:note>It is possible that hourly conversion rates differ on the step/cell level. If this is the case for one or more steps/cells, then add a salary table that describes the remuneration **per hour**.</sth:note>
                        </xs:documentation>
                      </xs:annotation>
                    </xs:element>
                  </xs:sequence>
                </xs:complexType>
              </xs:element>
              <xs:element name="salaryScale" maxOccurs="unbounded">
                <xs:annotation>
                  <xs:documentation>
                    <sth:definition>Base salary of a job or of an employee including minimum and maximum salary and certain salary steps. </sth:definition>
                    <sth:note>A salary scale can both contain a defined sequence of steps or be a scale with a minimum and maximum value are provided to indicate the range of the scale.</sth:note>
                  </xs:documentation>
                </xs:annotation>
                <xs:complexType>
                  <xs:sequence>
                    <xs:element name="name" type="xs:string">
                      <xs:annotation>
                        <xs:documentation>
                          <sth:definition>The name or label of the salary scale. E.g., 1,2,3</sth:definition>
                        </xs:documentation>
                      </xs:annotation>
                    </xs:element>
                    <xs:element name="maxValue" minOccurs="0">
                      <xs:annotation>
                        <xs:documentation>
                          <sth:definition>The maximum value of the salary scale.</sth:definition>
                        </xs:documentation>
                      </xs:annotation>
                      <xs:simpleType>
                        <xs:restriction base="xs:decimal"/>
                      </xs:simpleType>
                    </xs:element>
                    <xs:element name="minValue" minOccurs="0">
                      <xs:annotation>
                        <xs:documentation>
                          <sth:definition>The minimum value of the salary scale.</sth:definition>
                        </xs:documentation>
                      </xs:annotation>
                      <xs:simpleType>
                        <xs:restriction base="xs:decimal"/>
                      </xs:simpleType>
                    </xs:element>
                    <xs:element name="salaryStep" minOccurs="0" maxOccurs="unbounded">
                      <xs:annotation>
                        <xs:documentation>
                          <sth:definition> Specifies the steps within each salary scale, where each step has a name/label, a salary value, and possible conditions.</sth:definition>
                        </xs:documentation>
                      </xs:annotation>
                      <xs:complexType>
                        <xs:sequence>
                          <xs:element name="name" type="xs:string">
                            <xs:annotation>
                              <xs:documentation>
                                <sth:definition>The name or label of the step. E.g., 1,2,3</sth:definition>
                              </xs:documentation>
                            </xs:annotation>
                          </xs:element>
                          <xs:element name="value">
                            <xs:annotation>
                              <xs:documentation>
                                <sth:definition>The value of the step.</sth:definition>
                              </xs:documentation>
                            </xs:annotation>
                            <xs:simpleType>
                              <xs:restriction base="xs:decimal"/>
                            </xs:simpleType>
                          </xs:element>
                          <xs:element name="minimumWage" minOccurs="0" type="xs:boolean">
                            <xs:annotation>
                              <xs:documentation>
                                <sth:definition>A boolean that indicates whether this salary step is equal to the minimum wage. </sth:definition>
                              </xs:documentation>
                            </xs:annotation>
                          </xs:element>
                          <xs:element name="conditions" minOccurs="0" maxOccurs="unbounded">
                            <xs:annotation>
                              <xs:documentation>
                                <sth:definition>The conditions that apply to this salary step. E.g., age or any other condition</sth:definition>
                              </xs:documentation>
                            </xs:annotation>
                            <xs:complexType>
                              <xs:sequence>
                                <xs:element name="conditionType" type="xs:string">
                                  <xs:annotation>
                                    <xs:documentation>
                                      <sth:definition>A condition type element specifies the type of condition that influences the inclusion of various fields based on predefined criteria. The types of conditions are defined in our code list and currently include:

Function Condition: Related to specific job functions or roles.
Age Condition: Based on the age of the employee.
Employment Condition: Pertaining to employment terms and conditions.
Period Condition: Related to specific time periods.
General Condition: A broad category for general conditions.

Based on the condition type, different fields can be included.</sth:definition>
                                    </xs:documentation>
                                  </xs:annotation>
                                </xs:element>
                                <xs:any minOccurs="0" maxOccurs="unbounded"/>
                              </xs:sequence>
                            </xs:complexType>
                          </xs:element>
                        </xs:sequence>
                      </xs:complexType>
                    </xs:element>
                    <xs:element name="careerLevel" minOccurs="0">
                      <xs:annotation>
                        <xs:documentation>
                          <sth:definition>Indicates whether work experience is considered in the salary scale, with a true or false value and a possible description.</sth:definition>
                        </xs:documentation>
                      </xs:annotation>
                      <xs:complexType>
                        <xs:sequence>
                          <xs:element name="indicator" minOccurs="0" type="xs:boolean">
                            <xs:annotation>
                              <xs:documentation>
                                <sth:definition>A boolean indicating the presence of taking into account classification of career levels.</sth:definition>
                              </xs:documentation>
                            </xs:annotation>
                          </xs:element>
                          <xs:element name="description" minOccurs="0">
                            <xs:annotation>
                              <xs:documentation>
                                <sth:definition>Descriptions of the career levels.</sth:definition>
                              </xs:documentation>
                            </xs:annotation>
                            <xs:simpleType>
                              <xs:restriction base="xs:string"/>
                            </xs:simpleType>
                          </xs:element>
                        </xs:sequence>
                      </xs:complexType>
                    </xs:element>
                  </xs:sequence>
                </xs:complexType>
              </xs:element>
              <xs:element name="individualSalaryIncreaseRate" minOccurs="0" maxOccurs="unbounded">
                <xs:annotation>
                  <xs:documentation>
                    <sth:definition>The individual salary review is a process where an employee's salary is evaluated and adjusted based on individual performance, and other relevant criteria. This container specifies the increase rate of these adjustments.</sth:definition>
                  </xs:documentation>
                </xs:annotation>
                <xs:complexType>
                  <xs:sequence>
                    <xs:element name="expenseDate" minOccurs="0">
                      <xs:annotation>
                        <xs:documentation>
                          <sth:definition>This is formatted representation of a date, which is a date. This increase date is the specific date on which a salary adjustment or increase takes effect.</sth:definition>
                        </xs:documentation>
                      </xs:annotation>
                      <xs:simpleType>
                        <xs:restriction base="xs:date"/>
                      </xs:simpleType>
                    </xs:element>
                    <xs:element name="line" minOccurs="0">
                      <xs:annotation>
                        <xs:documentation>
                          <sth:definition>A container that contains the details of the increase rate. It includes elements such as the amount that is provided (3% of yearly salary), the interval in which this is applied in the specified period (e.g. each year) and any conditions that might have to be met for this rate to be applicable.</sth:definition>
                        </xs:documentation>
                      </xs:annotation>
                      <xs:complexType>
                        <xs:sequence>
                          <xs:element name="amount">
                            <xs:annotation>
                              <xs:documentation>
                                <sth:definition>
Container element that contains the value of the amount, the type of unit of the value and a base. These three elements together can be used to calculate the amount of any type of rate. If applicable, a minimum and a maximum value can be provided to signify a minimum and maximum that this rate provides. This implies that if the amount as a combination of the value and the base and the unit code is less than the minimum value, the minimum value will replace the value element.</sth:definition>
                              </xs:documentation>
                            </xs:annotation>
                            <xs:complexType>
                              <xs:sequence>
                                <xs:element name="value">
                                  <xs:annotation>
                                    <xs:documentation>
                                      <sth:definition>The numerical value of the increase expressed as an integer. The unit type of this value can be found in the unit code element. This unit code can be for example: 'euro' or 'percentage', this value element would then signify the value as expressed in a money equivalent 'euro' (fixed) or certain percentage value if the unit code would be 'percentage'.</sth:definition>
                                    </xs:documentation>
                                  </xs:annotation>
                                  <xs:simpleType>
                                    <xs:restriction base="xs:string"/>
                                  </xs:simpleType>
                                </xs:element>
                                <xs:element name="minValue" minOccurs="0">
                                  <xs:annotation>
                                    <xs:documentation>
                                      <sth:definition>
If applicable, a minimum value can be provided to signify a minimum that this rate provides. This implies that if the amount as a combination of the value and the base and the unit code is less than the minimum value, the minimum value will be used instead of the value element.</sth:definition>
                                    </xs:documentation>
                                  </xs:annotation>
                                  <xs:simpleType>
                                    <xs:restriction base="xs:decimal"/>
                                  </xs:simpleType>
                                </xs:element>
                                <xs:element name="maxValue" minOccurs="0">
                                  <xs:annotation>
                                    <xs:documentation>
                                      <sth:definition>
If applicable, a maximum value can be provided to signify a maximum that this rate provides. This implies that if the amount as a combination of the value and the base and the unit code is less than the maximum value, the maximum value will be used instead of the value element.</sth:definition>
                                    </xs:documentation>
                                  </xs:annotation>
                                  <xs:simpleType>
                                    <xs:restriction base="xs:decimal"/>
                                  </xs:simpleType>
                                </xs:element>
                                <xs:element name="unitCode">
                                  <xs:annotation>
                                    <xs:documentation>
                                      <sth:definition>
This element provides a way to specify the type of unit of the value. This can be for example 'percentage' if the unit type of the value is to be interpreted as a percentage or euro if the unity type of the value is to be interpreted as a euro monetary amount.</sth:definition>
                                    </xs:documentation>
                                  </xs:annotation>
                                  <xs:simpleType>
                                    <xs:restriction base="xs:string">
                                      <xs:enumeration value="euro"/>
                                      <xs:enumeration value="hour"/>
                                      <xs:enumeration value="percentage"/>
                                    </xs:restriction>
                                  </xs:simpleType>
                                </xs:element>
                                <xs:element name="baseAmount" minOccurs="0">
                                  <xs:annotation>
                                    <xs:documentation>
                                      <sth:definition>This element stores information about the base over which the amount is calculated. If the value is 3, the unit code is 'percentage' and the base code is 'MonthlySalary', together they would imply a 3% of monthly salary.</sth:definition>
                                    </xs:documentation>
                                  </xs:annotation>
                                  <xs:complexType>
                                    <xs:sequence>
                                      <xs:element name="unitCode">
                                        <xs:annotation>
                                          <xs:documentation>
                                            <sth:definition>
The type code of the base like for instance 'percentageMonthlyRate'.</sth:definition>
                                          </xs:documentation>
                                        </xs:annotation>
                                        <xs:simpleType>
                                          <xs:restriction base="xs:string">
                                            <xs:enumeration value="fixed"/>
                                            <xs:enumeration value="percentageFixedAmount"/>
                                            <xs:enumeration value="percentageHourlyRate"/>
                                            <xs:enumeration value="percentageMontlyRate"/>
                                            <xs:enumeration value="percentageOfExpense"/>
                                            <xs:enumeration value="percentageYearlyRate"/>
                                          </xs:restriction>
                                        </xs:simpleType>
                                      </xs:element>
                                      <xs:element name="value" minOccurs="0">
                                        <xs:annotation>
                                          <xs:documentation>
                                            <sth:definition>
This element can be used to store additional information about the value of the base if applicable. An example of this would be if the code of the base is "PercentageOfFixedAmount", this element could store the fixed amount.</sth:definition>
                                          </xs:documentation>
                                        </xs:annotation>
                                        <xs:simpleType>
                                          <xs:restriction base="xs:string"/>
                                        </xs:simpleType>
                                      </xs:element>
                                      <xs:element name="minValue" minOccurs="0">
                                        <xs:annotation>
                                          <xs:documentation>
                                            <sth:definition>
If applicable, a minimum value can be provided to signify a minimum that the value in this base has to be. This implies that if the value is less than the minimum value, the minimum value will be used instead of the value element.</sth:definition>
                                          </xs:documentation>
                                        </xs:annotation>
                                        <xs:simpleType>
                                          <xs:restriction base="xs:decimal"/>
                                        </xs:simpleType>
                                      </xs:element>
                                      <xs:element name="maxValue" minOccurs="0">
                                        <xs:annotation>
                                          <xs:documentation>
                                            <sth:definition>
If applicable, a maximum value can be provided to signify a maximum that the value in this base has to be. This implies that if the value is more than the maximum value, the maximum value will be used instead of the value element.</sth:definition>
                                          </xs:documentation>
                                        </xs:annotation>
                                        <xs:simpleType>
                                          <xs:restriction base="xs:decimal"/>
                                        </xs:simpleType>
                                      </xs:element>
                                    </xs:sequence>
                                  </xs:complexType>
                                </xs:element>
                              </xs:sequence>
                            </xs:complexType>
                          </xs:element>
                          <xs:element name="intervalCode">
                            <xs:annotation>
                              <xs:documentation>
                                <sth:definition>If applicable, this element can be used to signify the interval in which this rate is applied. If this rate is applied every hour then the interval code would be 'Hour' or if this rate is applied every kilometer, the interval code would be 'Kilometer'.</sth:definition>
                              </xs:documentation>
                            </xs:annotation>
                            <xs:complexType>
                              <xs:sequence>
                                <xs:element name="value" type="xs:string">
                                  <xs:annotation>
                                    <xs:documentation>
                                      <sth:definition>The value of the interval code.</sth:definition>
                                    </xs:documentation>
                                  </xs:annotation>
                                </xs:element>
                                <xs:element name="unitCode">
                                  <xs:annotation>
                                    <xs:documentation>
                                      <sth:definition>The value of the interval code.</sth:definition>
                                    </xs:documentation>
                                  </xs:annotation>
                                  <xs:simpleType>
                                    <xs:restriction base="xs:string">
                                      <xs:enumeration value="day"/>
                                      <xs:enumeration value="hour"/>
                                      <xs:enumeration value="month"/>
                                      <xs:enumeration value="week"/>
                                      <xs:enumeration value="year"/>
                                    </xs:restriction>
                                  </xs:simpleType>
                                </xs:element>
                              </xs:sequence>
                            </xs:complexType>
                          </xs:element>
                          <xs:element name="conditions" minOccurs="0" maxOccurs="unbounded">
                            <xs:annotation>
                              <xs:documentation>
                                <sth:definition>
A container element for providing more information about the conditions that have to be met before this rate is applied. An employee that meets these conditions is eligible for the rate.</sth:definition>
                              </xs:documentation>
                            </xs:annotation>
                            <xs:complexType>
                              <xs:sequence>
                                <xs:element name="conditionType" type="xs:string">
                                  <xs:annotation>
                                    <xs:documentation>
                                      <sth:definition>A condition type element specifies the type of condition that influences the predefined criteria. The types of conditions are defined in our code list and currently include:

Function Condition: Related to specific job functions or roles.
Age Condition: Based on the age of the employee.
Employment Condition: Pertaining to employment terms and conditions.
Period Condition: Related to specific time periods.
General Condition: A broad category for general conditions.

Based on the condition type, different fields can be included to ensure accurate and relevant data handling.</sth:definition>
                                    </xs:documentation>
                                  </xs:annotation>
                                </xs:element>
                                <xs:any minOccurs="0" maxOccurs="unbounded"/>
                              </xs:sequence>
                            </xs:complexType>
                          </xs:element>
                        </xs:sequence>
                      </xs:complexType>
                    </xs:element>
                  </xs:sequence>
                </xs:complexType>
              </xs:element>
              <xs:element name="generalSalaryIncreaseRate" minOccurs="0" maxOccurs="unbounded">
                <xs:annotation>
                  <xs:documentation>
                    <sth:definition>The general salary adjustment refers to the increase applied uniformly to the entire salary table. This adjustment ensures that all employees receive a consistent percentage or fixed increase in their salaries. Please mind the usage note when to use this container.</sth:definition>
                    <sth:note>Initial increases are adjustments to the entire salary structure. For example, everyone receives a 4% increase, which changes the salary scales and steps. As a result, a new salary table will eventually be created. This is not always done in practice. To still indicate what the initial increases will be the initial increase block can be used.</sth:note>
                  </xs:documentation>
                </xs:annotation>
                <xs:complexType>
                  <xs:sequence>
                    <xs:element name="effectivePeriod" minOccurs="0">
                      <xs:annotation>
                        <xs:documentation>
                          <sth:definition>The effective start date of this initial increase.</sth:definition>
                        </xs:documentation>
                      </xs:annotation>
                      <xs:complexType>
                        <xs:sequence>
                          <xs:element name="validFrom">
                            <xs:annotation>
                              <xs:documentation>
                                <sth:definition>The effective start date of this initial increase.</sth:definition>
                              </xs:documentation>
                            </xs:annotation>
                            <xs:simpleType>
                              <xs:restriction base="xs:date"/>
                            </xs:simpleType>
                          </xs:element>
                        </xs:sequence>
                      </xs:complexType>
                    </xs:element>
                    <xs:element name="amount">
                      <xs:annotation>
                        <xs:documentation>
                          <sth:definition>
Container element that contains the value of the amount and the type of unit of the value. If applicable, a minimum and a maximum value can also be provided to signify a minimum and maximum that this rate provides. The base of this amount is the salary table provided within the remuneration container.</sth:definition>
                        </xs:documentation>
                      </xs:annotation>
                      <xs:complexType>
                        <xs:sequence>
                          <xs:element name="value">
                            <xs:annotation>
                              <xs:documentation>
                                <sth:definition>
The numerical value of the initial increase expressed as a number. The unit type of this value can be found in the unit code element. This unit code can be for example: 'fixed' or 'percentage', this value element would then signify the value as expressed in a money equivalent 'fixed' or certain percentage value if the unit code would be 'percentage'.</sth:definition>
                              </xs:documentation>
                            </xs:annotation>
                            <xs:simpleType>
                              <xs:restriction base="xs:string"/>
                            </xs:simpleType>
                          </xs:element>
                          <xs:element name="unitCode">
                            <xs:annotation>
                              <xs:documentation>
                                <sth:definition>
This element provides a way to specify the type of unit of the value. This can be for example 'percentage' if the unit type of the value is to be interpreted as a percentage or euro if the unity type of the value is to be interpreted as a euro monetary amount.</sth:definition>
                              </xs:documentation>
                            </xs:annotation>
                            <xs:simpleType>
                              <xs:restriction base="xs:string">
                                <xs:enumeration value="euro"/>
                                <xs:enumeration value="percentage"/>
                              </xs:restriction>
                            </xs:simpleType>
                          </xs:element>
                        </xs:sequence>
                      </xs:complexType>
                    </xs:element>
                    <xs:element name="description" minOccurs="0" type="xs:string">
                      <xs:annotation>
                        <xs:documentation>
                          <sth:definition>The free text description of the general salary table increase</sth:definition>
                        </xs:documentation>
                      </xs:annotation>
                    </xs:element>
                  </xs:sequence>
                </xs:complexType>
              </xs:element>
              <xs:element name="conditions" minOccurs="0" maxOccurs="unbounded">
                <xs:annotation>
                  <xs:documentation>
                    <sth:definition>
A container element for providing more information about the conditions that have to be met before this remuneration applies.</sth:definition>
                  </xs:documentation>
                </xs:annotation>
                <xs:complexType>
                  <xs:sequence>
                    <xs:element name="conditionType" type="xs:string">
                      <xs:annotation>
                        <xs:documentation>
                          <sth:definition>A condition type element specifies the type of condition that influences the inclusion of various fields based on predefined criteria. The types of conditions are defined in our code list and currently include:

Function Condition: Related to specific job functions or roles.
Age Condition: Based on the age of the employee.
Employment Condition: Pertaining to employment terms and conditions.
Period Condition: Related to specific time periods.
General Condition: A broad category for general conditions.

Based on the condition type, different fields can be included.</sth:definition>
                        </xs:documentation>
                      </xs:annotation>
                    </xs:element>
                    <xs:any minOccurs="0" maxOccurs="unbounded"/>
                  </xs:sequence>
                </xs:complexType>
              </xs:element>
            </xs:sequence>
          </xs:complexType>
        </xs:element>
        <xs:element name="allowance" minOccurs="0" maxOccurs="unbounded">
          <xs:annotation>
            <xs:documentation>
              <sth:definition>A container element that includes various allowances that can be granted to an employee.</sth:definition>
            </xs:documentation>
          </xs:annotation>
          <xs:complexType>
            <xs:sequence>
              <xs:element name="id" minOccurs="0">
                <xs:annotation>
                  <xs:documentation>
                    <sth:definition>A unique identifier which cannot be expressed as enumeration</sth:definition>
                  </xs:documentation>
                </xs:annotation>
                <xs:complexType>
                  <xs:sequence>
                    <xs:element name="value">
                      <xs:annotation>
                        <xs:documentation>
                          <sth:definition>The identifier.</sth:definition>
                        </xs:documentation>
                      </xs:annotation>
                      <xs:simpleType>
                        <xs:restriction base="xs:string"/>
                      </xs:simpleType>
                    </xs:element>
                    <xs:element name="schemeAgencyId">
                      <xs:annotation>
                        <xs:documentation>
                          <sth:definition>The organization that issues the identifier. This organization is also responsible for unicity of the identifier within its own context.</sth:definition>
                        </xs:documentation>
                      </xs:annotation>
                      <xs:simpleType>
                        <xs:restriction base="xs:string">
                          <xs:enumeration value="Customer"/>
                          <xs:enumeration value="Supplier"/>
                        </xs:restriction>
                      </xs:simpleType>
                    </xs:element>
                  </xs:sequence>
                </xs:complexType>
              </xs:element>
              <xs:element name="name">
                <xs:annotation>
                  <xs:documentation>
                    <sth:definition>The name or description of the allowance, indicating the type of allowance and giving extra information that does not fit in any of the other fields.</sth:definition>
                  </xs:documentation>
                </xs:annotation>
                <xs:simpleType>
                  <xs:restriction base="xs:string"/>
                </xs:simpleType>
              </xs:element>
              <xs:element name="effectivePeriod" minOccurs="0">
                <xs:annotation>
                  <xs:documentation>
                    <sth:definition>The time period from the start date up and including to the end date to indicate the validity of this allowance regulation. </sth:definition>
                  </xs:documentation>
                </xs:annotation>
                <xs:complexType>
                  <xs:sequence>
                    <xs:element name="validFrom" minOccurs="0">
                      <xs:annotation>
                        <xs:documentation>
                          <sth:definition>The start date of the validity period of this allowance</sth:definition>
                        </xs:documentation>
                      </xs:annotation>
                      <xs:simpleType>
                        <xs:restriction base="xs:string"/>
                      </xs:simpleType>
                    </xs:element>
                    <xs:element name="validTo" minOccurs="0">
                      <xs:annotation>
                        <xs:documentation>
                          <sth:definition>The end date of the validity period of this allowance. </sth:definition>
                        </xs:documentation>
                      </xs:annotation>
                      <xs:simpleType>
                        <xs:restriction base="xs:string"/>
                      </xs:simpleType>
                    </xs:element>
                  </xs:sequence>
                </xs:complexType>
              </xs:element>
              <xs:element name="typeCode">
                <xs:annotation>
                  <xs:documentation>
                    <sth:definition>A code specifying the type of allowance, such as ORT (irregularity allowance) or physical burden. There is a codelist with predefined allowances.</sth:definition>
                    <sth:note>  
Paid breaks and rest periods will be included in this allowance block. There is an HT400 (hour type) code for defining breaks. If no breaks are included in the allowance block, it means that there are no paid breaks, and the breaks are therefore unpaid.</sth:note>
                  </xs:documentation>
                </xs:annotation>
                <xs:complexType>
                  <xs:sequence>
                    <xs:element name="value" type="xs:string">
                      <xs:annotation>
                        <xs:documentation>
                          <sth:definition>The value of the code.</sth:definition>
                          <sth:note>  
Paid breaks and rest periods will be included in this allowance block. There is an HT400 (hour type) code for defining breaks. If no breaks are included in the allowance block, it means that there are no paid breaks, and the breaks are therefore unpaid.</sth:note>
                        </xs:documentation>
                      </xs:annotation>
                    </xs:element>
                  </xs:sequence>
                </xs:complexType>
              </xs:element>
              <xs:element name="period" minOccurs="0" maxOccurs="unbounded">
                <xs:annotation>
                  <xs:documentation>
                    <sth:definition>A container element that specifies the time period(s) during which the allowance is applicable. It includes a date period (which can be used for date ranges or individual dates), a time period (given the selected dates, this lets us specify the allowed times) and a weekday element that lets us specify recurring days of the week. 

</sth:definition>
                    <sth:note>Dit JSON-voorbeeld beschrijft een onregelmatigheidstoeslag genaamd "Onregelmatigheidstoeslag zomer" die geldig is van 1 juli 2025 tot en met 31 december 2025. De toeslag is van toepassing op maandagen, dinsdagen en woensdagen tussen 18:00 en 23:00 uur.</sth:note>
                  </xs:documentation>
                </xs:annotation>
                <xs:complexType>
                  <xs:sequence>
                    <xs:element name="datePeriod" minOccurs="0" maxOccurs="unbounded">
                      <xs:annotation>
                        <xs:documentation>
                          <sth:definition>Single period spanning from the start date up and including to the end date. If both start and end is provided, the date period element indicates a range of dates. If only the start date is provided, the date period element only indicates this singular date.</sth:definition>
                        </xs:documentation>
                      </xs:annotation>
                      <xs:complexType>
                        <xs:sequence>
                          <xs:element name="start">
                            <xs:annotation>
                              <xs:documentation>
                                <sth:definition>Date representation according ISO 8601 standard.</sth:definition>
                              </xs:documentation>
                            </xs:annotation>
                            <xs:simpleType>
                              <xs:restriction base="xs:date"/>
                            </xs:simpleType>
                          </xs:element>
                          <xs:element name="end" minOccurs="0">
                            <xs:annotation>
                              <xs:documentation>
                                <sth:definition>Date representation according ISO 8601 standard.</sth:definition>
                              </xs:documentation>
                            </xs:annotation>
                            <xs:simpleType>
                              <xs:restriction base="xs:date"/>
                            </xs:simpleType>
                          </xs:element>
                        </xs:sequence>
                      </xs:complexType>
                    </xs:element>
                    <xs:element name="timePeriod">
                      <xs:annotation>
                        <xs:documentation>
                          <sth:definition>The time period during which the allowance is applicable. This period spans from the start time up and including to the end time. 

In the event that multiple time periods are required, multiple period elements can be provided.</sth:definition>
                        </xs:documentation>
                      </xs:annotation>
                      <xs:complexType>
                        <xs:sequence>
                          <xs:element name="start">
                            <xs:annotation>
                              <xs:documentation>
                                <sth:definition>The start date to which the payrate is valid</sth:definition>
                              </xs:documentation>
                            </xs:annotation>
                            <xs:simpleType>
                              <xs:restriction base="xs:string"/>
                            </xs:simpleType>
                          </xs:element>
                          <xs:element name="end">
                            <xs:annotation>
                              <xs:documentation>
                                <sth:definition>
The end date  to which the payrate is valid</sth:definition>
                              </xs:documentation>
                            </xs:annotation>
                            <xs:simpleType>
                              <xs:restriction base="xs:string"/>
                            </xs:simpleType>
                          </xs:element>
                        </xs:sequence>
                      </xs:complexType>
                    </xs:element>
                    <xs:element name="weekday" minOccurs="0" maxOccurs="unbounded">
                      <xs:annotation>
                        <xs:documentation>
                          <sth:definition>The day in the week to which the allowance is applicable</sth:definition>
                        </xs:documentation>
                      </xs:annotation>
                      <xs:simpleType>
                        <xs:restriction base="xs:string">
                          <xs:enumeration value="Friday"/>
                          <xs:enumeration value="Monday"/>
                          <xs:enumeration value="Saturday"/>
                          <xs:enumeration value="Sunday"/>
